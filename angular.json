{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "./projects",
  "projects": {
    "ngx-utilities-app-client": {
      "root": "./",
      "sourceRoot": "./src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "./dist/app",
            "index": "./src/index.html",
            "main": "./src/main.ts",
            "polyfills": "./src/polyfills.ts",
            "tsConfig": "./src/tsconfig.app.json",
            "assets": ["./src/favicon.ico", "./src/assets", { "glob": "**/*.md", "input": "./projects", "output": "./projects" }],
            "styles": ["./src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        }
      }
    },
    "ngx-utilities-app-server": {
      "root": "./",
      "sourceRoot": "./src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "./dist/server",
            "main": "./src/main.server.ts",
            "tsConfig": "./src/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "./src/environments/environment.ts",
                  "with": "./src/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    },
    "ngx-utilities-app-karma": {
      "root": "",
      "projectType": "application",
      "architect": {
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "./src/test.ts",
            "polyfills": "./src/polyfills.ts",
            "tsConfig": "./src/tsconfig.spec.json",
            "karmaConfig": "./src/karma.conf.js"
          }
        }
      }
    },
    "ngx-utilities-app-e2e": {
      "root": "./e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./e2e/protractor.conf.js"
          }
        }
      }
    },
    "@ngx-utilities/http-retry": {
      "root": "./projects/http-retry",
      "sourceRoot": "./projects/http-retry/src",
      "projectType": "library",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "./projects/tsconfig.lib.json",
            "project": "./projects/http-retry/ng-package.json"
          }
        }
      }
    },
    "@ngx-utilities/ngx-let": {
      "root": "./projects/ngx-let",
      "sourceRoot": "./projects/ngx-let/src",
      "projectType": "library",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "./projects/tsconfig.lib.json",
            "project": "./projects/ngx-let/ng-package.json"
          }
        }
      }
    },
    "@ngx-utilities/ngx-if-else-loading": {
      "root": "./projects/ngx-if-else-loading",
      "sourceRoot": "./projects/ngx-if-else-loading/src",
      "projectType": "library",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "./projects/tsconfig.lib.json",
            "project": "./projects/ngx-if-else-loading/ng-package.json"
          }
        }
      }
    },
    "@ngx-utilities/observe-property": {
      "root": "./projects/observe-property",
      "sourceRoot": "./projects/observe-property/src",
      "projectType": "library",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "./projects/tsconfig.lib.json",
            "project": "./projects/observe-property/ng-package.json"
          }
        }
      }
    },
    "@ngx-utilities/ngx-transfer-state": {
      "root": "./projects/ngx-transfer-state",
      "sourceRoot": "./projects/ngx-transfer-state/src",
      "projectType": "library",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "./projects/tsconfig.lib.json",
            "project": "./projects/ngx-transfer-state/ng-package.json"
          }
        }
      }
    }
  },
  "defaultProject": "ngx-utilities-app-client"
}
